{"ast":null,"code":"import DEMO_JSON from \"../json/demo.json\";\nimport 'vue3-carousel/dist/carousel.css';\nimport { Carousel, Slide, Pagination, Navigation } from 'vue3-carousel';\nexport default {\n  mounted() {\n    console.log(DEMO_JSON[this.$route.params.software][0].features);\n  },\n\n  components: {\n    Carousel,\n    Slide,\n    Pagination,\n    Navigation\n  },\n\n  data() {\n    return {\n      'software_name': this.$route.params.software,\n      'header': [],\n      'navbar': [],\n      'screenshots': [],\n      'features': [],\n      'pricing': [],\n      'testimonials': []\n    };\n  },\n\n  methods: {\n    getHeader() {\n      this.header = DEMO_JSON[this.$route.params.software][0].header;\n    },\n\n    getNavbar() {\n      this.navbar = DEMO_JSON.navbar;\n    },\n\n    getScreenshots() {\n      this.screenshots = DEMO_JSON[this.$route.params.software][0].screenshots;\n    },\n\n    getFeatures() {\n      this.features = DEMO_JSON[this.$route.params.software][0].features;\n    },\n\n    getPricing() {\n      this.pricing = DEMO_JSON[this.$route.params.software][0].pricing;\n    },\n\n    getTestimonials() {\n      this.testimonials = DEMO_JSON[this.$route.params.software][0].testimonials;\n    }\n\n  },\n\n  created() {\n    this.getHeader();\n    this.getNavbar();\n    this.getScreenshots();\n    this.getFeatures();\n    this.getPricing();\n    this.getTestimonials();\n  }\n\n};","map":{"version":3,"mappings":"AAybA,OAAOA,SAAP,MAAsB,mBAAtB;AAEA,OAAO,iCAAP;AACA,SAASC,QAAT,EAAmBC,KAAnB,EAA0BC,UAA1B,EAAsCC,UAAtC,QAAwD,eAAxD;AAEA,eAAe;EACbC,OAAO,GAAG;IACRC,OAAO,CAACC,GAAR,CAAYP,SAAS,CAAC,KAAKQ,MAAL,CAAYC,MAAZ,CAAmBC,QAApB,CAAT,CAAuC,CAAvC,EAA0CC,QAAtD;EACD,CAHY;;EAIbC,UAAU,EAAE;IACVX,QADU;IAEVC,KAFU;IAGVC,UAHU;IAIVC;EAJU,CAJC;;EAUbS,IAAI,GAAG;IACL,OAAO;MACL,iBAAiB,KAAKL,MAAL,CAAYC,MAAZ,CAAmBC,QAD/B;MAEL,UAAU,EAFL;MAGL,UAAU,EAHL;MAIL,eAAe,EAJV;MAKL,YAAY,EALP;MAML,WAAY,EANP;MAOL,gBAAgB;IAPX,CAAP;EASD,CApBY;;EAqBbI,OAAO,EAAE;IACPC,SAAS,GAAG;MACV,KAAKC,MA